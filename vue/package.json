{
  "name": "e2e",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "serve:e2e": "(cd ../phoenix; MIX_ENV=test E2E=1 SANDBOX=1 mix phx.server)",
    "serve:e2e:ci": "(cd ../phoenix; MIX_ENV=test E2E=1 SANDBOX=1 HEADLESS=1 mix phx.server)",
    "test:e2e": "(npm run serve:e2e & vue-cli-service test:e2e && kill $(lsof -t -i:4000))",
    "test:e2e:ci": "(npm run serve:e2e:ci & vue-cli-service test:e2e --headless && kill $(lsof -t -i:4000))",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "eslint-import-resolver-webpack": "~0.13",
    "vue": "^2.6.10",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.16.1",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "~5.48",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-e2e-cypress": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-airbnb": "~7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "eslint": "~8.2.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "sass": "^1.44.0",
    "sass-loader": "^13.2.0",
    "typescript": "~4.0",
    "vue-template-compiler": "^2.6.10"
  }
}
