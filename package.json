{
  "name": "e2e-recipes",
  "version": "0.0.1",
  "description": "Collection of e2e testing recipes",
  "main": "index.js",
  "scripts": {
    "lint:phoenix:credo": "(cd phoenix; mix credo)",
    "lint:phoenix:formatter": "(cd phoenix; mix format --check-formatted)",
    "lint:phoenix:dialyzer": "(cd phoenix; mix dialyzer)",
    "lint:angular": "(cd angular; npm run lint)",
    "lint:react": "(cd react; npm run lint)",
    "lint:vue": "(cd vue; npm run lint)",
    "start:angular": "(cd angular; ng serve --open)",
    "start:ember": "(cd ember; npm run start)",
    "start:phoenix": "(cd phoenix; mix phx.server)",
    "start:react": "(cd react; npm run start)",
    "start:vue": "(cd vue; npm run serve)",
    "test:ci:angular": "(cd angular; npm run test:e2e:ci)",
    "test:ci:phoenix": "(cd phoenix; mix ci)",
    "test:ci:react": "(cd react; npm run test:e2e:ci)",
    "test:ci:vue": "(cd vue; npm run test:e2e:ci)",
    "test:e2e:angular": "(cd angular; npm run test:e2e)",
    "test:e2e:phoenix": "(cd phoenix; mix acceptance)",
    "test:e2e:react": "(cd react; npm run test:e2e)",
    "test:e2e:vue": "(cd vue; npm run test:e2e)",
    "test:unit:phoenix": "(cd phoenix; mix test)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bego-talks/e2e.git"
  },
  "keywords": [
    "e2e",
    "test",
    "recipe"
  ],
  "author": "Nikola Begedin (begedinnikola@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bego-talks/e2e/issues"
  },
  "homepage": "https://github.com/bego-talks/e2e#readme"
}
